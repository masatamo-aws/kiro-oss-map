{
  "name": "kiro-oss-map",
  "version": "1.0.0",
  "description": "Open Source Map Web Application using OpenStreetMap",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3003",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "serve": "node server/index.js",
    "error-free": "echo 'Starting error-free Kiro OSS Map...' && npm run dev"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "maplibre-gl": "^3.6.2",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "express-rate-limit": "^8.0.1",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.0.4",
    "workbox-cli": "^7.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "keywords": [
    "map",
    "openstreetmap",
    "maplibre",
    "pwa",
    "routing",
    "geocoding"
  ],
  "author": "Kiro OSS Map Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiro-oss/kiro-oss-map.git"
  }
}